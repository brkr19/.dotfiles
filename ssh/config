Host *
 IdentityFile ~/.ssh/id_ecdsa
 IdentityFile ~/.ssh/id_ed25519
 IdentityFile ~/.ssh/id_rsa
 AddKeysToAgent yes
 IgnoreUnknown UseKeychain
 UseKeychain yes
 TCPKeepAlive no
 ServerAliveInterval 30
 ServerAliveCountMax 3
 ForwardX11 yes
 IPQoS lowdelay throughput
 ForwardAgent yes

Include ~/.ssh/config.d/*
